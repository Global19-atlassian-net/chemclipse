<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.chemclipse.rcp.app.ui.preferences.settingsPreferencePage"
            class="org.eclipse.chemclipse.swt.ui.preferences.SWTPreferencePage"
            id="org.eclipse.chemclipse.chromatogram.ui.swt.perspectivePreferencePage"
            name="Editor/View Preference Page">
      </page>
      <page
            category="org.eclipse.chemclipse.rcp.app.ui.preferences.settingsPreferencePage"
            class="org.eclipse.chemclipse.swt.ui.preferences.ChromatogramPreferencePage"
            id="org.eclipse.chemclipse.chromatogram.ui.swt.chromatogramPreferencePage"
            name="Specific Chromatogram Settings">
      </page>
      <page
            category="org.eclipse.chemclipse.rcp.app.ui.preferences.settingsPreferencePage"
            class="org.eclipse.chemclipse.swt.ui.preferences.PreferencePageSWT"
            id="org.eclipse.chemclipse.chromatogram.ui.swt.preferencePageSWT"
            name="Settings (SWT)">
      </page>            
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.eclipse.chemclipse.swt.ui.preferences.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
