<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         point="org.eclipse.chemclipse.rcp.app.ui.commandLineProcessor">
      <CommandLineProcessor
            description="Add a MSD file to the processing context"
            hasArguments="true"
            id="org.eclipse.chemclipse.cli.reader.msd.CommandLineProcessor"
            option="readmsd"
            processor="org.eclipse.chemclipse.cli.MsdReaderCommandLineProcessor">
      </CommandLineProcessor>
   </extension>
   <extension
         point="org.eclipse.chemclipse.rcp.app.ui.commandLineProcessor">
      <CommandLineProcessor
            description="Add a CSD file to the processing context"
            hasArguments="true"
            id="org.eclipse.chemclipse.cli.reader.csd.CommandLineProcessor"
            option="readcsd"
            processor="org.eclipse.chemclipse.cli.CsdReaderCommandLineProcessor">
      </CommandLineProcessor>
   </extension>
   <extension
         point="org.eclipse.chemclipse.rcp.app.ui.commandLineProcessor">
      <CommandLineProcessor
            description="Add a WSD file to the processing context"
            hasArguments="true"
            id="org.eclipse.chemclipse.cli.reader.wsd.CommandLineProcesso."
            option="readwsd"
            processor="org.eclipse.chemclipse.cli.WsdReaderCommandLineProcessor">
      </CommandLineProcessor>
   </extension>
   <extension
         point="org.eclipse.chemclipse.rcp.app.ui.commandLineProcessor">
      <CommandLineProcessor
            description="Applies a method to all current Chromatograms"
            hasArguments="true"
            id="org.eclipse.chemclipse.cli.method.CommandLineProcessor"
            option="method"
            processor="org.eclipse.chemclipse.cli.MethodCommandLineProcessor">
      </CommandLineProcessor>
   </extension>
   <extension
         id="org.eclipse.chemclipse.cli.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi"
            apply="always">
      </fragment>
   </extension>

</plugin>
