<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.eclipse.chemclipse.ux.extension.ui.modelContribution"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.eclipse.chemclipse.ux.extension.ui.definitions.isChromatogramTypeMSD">
         <with
               variable="org.eclipse.chemclipse.ux.extension.ui.definitions.chromatogramType">
            <equals
                  value="CHROMATOGRAM_TYPE_MSD">
            </equals>
         </with>
      </definition>
      <definition
            id="org.eclipse.chemclipse.ux.extension.ui.definitions.isChromatogramTypeCSD">
         <with
               variable="org.eclipse.chemclipse.ux.extension.ui.definitions.chromatogramType">
            <equals
                  value="CHROMATOGRAM_TYPE_CSD">
            </equals>
         </with>
      </definition>
      <definition
            id="org.eclipse.chemclipse.ux.extension.ui.definitions.isChromatogramTypeWSD">
         <with
               variable="org.eclipse.chemclipse.ux.extension.ui.definitions.chromatogramType">
            <equals
                  value="CHROMATOGRAM_TYPE_WSD">
            </equals>
         </with>
      </definition>
      <definition
            id="org.eclipse.chemclipse.ux.extension.ui.definitions.isChromatogramTypeXXD">
         <with
               variable="org.eclipse.chemclipse.ux.extension.ui.definitions.chromatogramType">
            <or>
               <equals
                     value="CHROMATOGRAM_TYPE_MSD">
               </equals>
               <equals
                     value="CHROMATOGRAM_TYPE_CSD">
               </equals>
               <equals
                     value="CHROMATOGRAM_TYPE_WSD">
               </equals>
            </or>
         </with>
      </definition>
      <definition
            id="org.eclipse.chemclipse.ux.extension.ui.definitions.isChromatogramTypeNone">
         <with
               variable="org.eclipse.chemclipse.ux.extension.ui.definitions.chromatogramType">
            <equals
                  value="CHROMATOGRAM_TYPE_NONE">
            </equals>
         </with>
      </definition>                        
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.eclipse.chemclipse.ux.extension.ui.definitions.isScanTypeMSD">
         <with
               variable="org.eclipse.chemclipse.ux.extension.ui.definitions.scanType">
            <equals
                  value="SCAN_TYPE_MSD">
            </equals>
         </with>
      </definition>
      <definition
            id="org.eclipse.chemclipse.ux.extension.ui.definitions.isScanTypeNone">
         <with
               variable="org.eclipse.chemclipse.ux.extension.ui.definitions.scanType">
            <equals
                  value="SCAN_TYPE_NONE">
            </equals>
         </with>
      </definition>                        
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.eclipse.chemclipse.ux.extension.ui.definitions.isLibraryTypeMSD">
         <with
               variable="org.eclipse.chemclipse.ux.extension.ui.definitions.libraryType">
            <equals
                  value="LIBRARY_TYPE_MSD">
            </equals>
         </with>
      </definition>
      <definition
            id="org.eclipse.chemclipse.ux.extension.ui.definitions.isLibraryTypeNone">
         <with
               variable="org.eclipse.chemclipse.ux.extension.ui.definitions.libraryType">
            <equals
                  value="LIBRARY_TYPE_NONE">
            </equals>
         </with>
      </definition>                        
   </extension>      
   
   
</plugin>
